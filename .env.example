# AI Agent Prompt Tuning Tool - API Keys
# Copy this to .env and fill in your actual API keys

# Anthropic (Claude) - Required for Phase 1 and Phase 3
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google (Gemini) - Required for Phase 2
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI (GPT) - Optional, for alternative configurations
OPENAI_API_KEY=your_openai_api_key_here

# Configuration Options
PROMPT_TUNING_VERBOSE=false
PROMPT_TUNING_SAVE_INTERMEDIATE=true
PROMPT_TUNING_MAX_ITERATIONS=20
PROMPT_TUNING_TARGET_SCORE=4.0
PROMPT_TUNING_STRETCH_SCORE=4.5

# API Rate Limiting (requests per minute)
ANTHROPIC_RPM_LIMIT=50
GOOGLE_RPM_LIMIT=60
OPENAI_RPM_LIMIT=50
